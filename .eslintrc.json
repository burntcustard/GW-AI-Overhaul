{
  "parserOptions": {
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "browser": true,
    "jquery": true,
    "amd": true
  },
  "extends": ["eslint:recommended", "plugin:lodash/v3", "prettier"],
  "globals": {
    "api": "readonly",
    "model": "writable",
    "_": "readonly",
    "loc": "readonly",
    "ko": "readonly",
    "requireGW": "readonly",
    "star_system_templates": "readonly",
    "parse": "readonly",
    "loadScript": "readonly",
    "createjs": "readonly",
    "loadHtml": "readonly",
    "locTree": "readonly",
    "Build": "readonly",
    "globals": "readonly",
    "app": "readonly",
    "engine": "readonly",
    "CommanderUtility": "readonly"
  },
  "plugins": ["lodash"],
  "rules": {
    "lodash/prefer-lodash-method": 0,
    "lodash/prefer-noop": 0,
    "no-inner-declarations": 0
  }
}
